{
  "$id": "http://kf-ui-data-tracker.kidsfirstdrc.org/src/analyticsTracking/event_schemas/DROPDOWN/DROPDOWN_EVENT.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DROPDOWN_EVENT base schema",
  "type": "object",
  "description": "onChange event for Dropdown components",
  "definitions": {
    "placeholder": {
      "$id": "#placeholder",
      "type": "string",
      "description": "HTML placeholder attribute"
    },
    "value": {
      "$id": "#value",
      "type": "string",
      "description": "the value that the input was changed to"
    },
    "results_length": {
      "$id": "#results_length",
      "type": "number",
      "description": "if filtering a list, the length of the list after the filter is applied"
    }
  },
  "properties": {
    "path": {
      "$ref": "http://kf-ui-data-tracker.kidsfirstdrc.org/src/analyticsTracking/event_schemas/common_defs.schema.json#/definitions/path"
    },
    "route": {
      "$ref": "http://kf-ui-data-tracker.kidsfirstdrc.org/src/analyticsTracking/event_schemas/common_defs.schema.json#/definitions/route"
    },
    "view": {
      "$ref": "http://kf-ui-data-tracker.kidsfirstdrc.org/src/analyticsTracking/event_schemas/common_defs.schema.json#/definitions/view"
    },
    "study": {
      "$ref": "http://kf-ui-data-tracker.kidsfirstdrc.org/src/analyticsTracking/event_schemas/common_defs.schema.json#/definitions/study"
    },
    "placeholder": {
      "$ref": "#placeholder"
    },
    "value": {
      "$ref": "#value"
    },
    "results_length": {
      "$ref": "#results_length"
    }
  }
}
